                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
	clt.Office as business_unit
	,client.id AS client_id
	,ent.number AS client_number
	,ent.name AS client_name
	,debtor.id AS debtor_id
	,d_ent.number AS debtor_number
	,d_ent.name AS debtor_name
	,inv.invno as invoice_number
	,inv.TransPostDate transpostdate
	,inv.amt as invoice_amount
	,inv.balance as invoice_balance
	,DATEDIFF(day,inv.TransPostDate,CURRENT_TIMESTAMP) as age
	,CASE WHEN inv.DebtorKey IN (11352,30273) THEN 'ADVANCE' ELSE 'INVOICE' END type
	,CURRENT_TIMESTAMP as row_created_date
	,CAST(CURRENT_TIMESTAMP as date) AS date
	,CASE
		WHEN DATEDIFF(day,inv.TransPostDate,CURRENT_TIMESTAMP) &lt;= 30 THEN '&lt;=30'
		WHEN DATEDIFF(day,inv.TransPostDate,CURRENT_TIMESTAMP) &lt;= 60 THEN '31-60'
		WHEN DATEDIFF(day,inv.TransPostDate,CURRENT_TIMESTAMP) &lt;= 90 THEN '61-90'
		WHEN DATEDIFF(day,inv.TransPostDate,CURRENT_TIMESTAMP) &lt;= 120 THEN '91-120'
		WHEN DATEDIFF(day,inv.TransPostDate,CURRENT_TIMESTAMP) &gt; 120 THEN '121+'
	END AS ar_bucket
	,client.row_source_system

FROM CadenceReplica.dbo.Invoices (nolock) inv
	INNER JOIN CadenceReplica.dbo.Clients clt (nolock) on inv.clientkey = clt.clientkey
	INNER JOIN CadenceReplica.dbo.Debtors dbt (nolock) on inv.debtorkey = dbt.debtorkey
	INNER JOIN dbo.client ON clt.clientkey = client.row_source_system_key
		AND client.row_source_system = 'Cadence'
	INNER JOIN dbo.entity ent ON client.entity_id = ent.id
	INNER JOIN dbo.debtor ON dbt.DebtorKey = debtor.row_source_system_key
		AND debtor.row_source_system = 'Cadence'
	INNER JOIN dbo.entity d_ent ON debtor.entity_id = d_ent.id

WHERE
	inv.balance &lt;&gt; 0
	AND inv.amt &lt;&gt; 0
	AND inv.BuyStatus = 3
	and clt.ClientKey &lt;&gt; 10704
	and clt.office = 'TBSFACTORING'

UNION ALL

SELECT
	'TBSCAPFUND' business
	,cli.id AS client_id
	,c_entity.number AS client_number
	,c_entity.name AS client_name
	,dbt.id AS debtor_id
	,ent.number AS debtor_number
	,ent.name AS debtor_name
	,i.tranid invoice_number
	,i.trandate transpostdate
	,foreigntotal inovice_amount
	,foreignamountunpaid invoice_balance
	,datediff(day,i.trandate,CURRENT_TIMESTAMP) age
	,'INVOICE' as type
	,CURRENT_TIMESTAMP as row_created_date
	,CAST(CURRENT_TIMESTAMP as date) AS date
	,CASE
		WHEN DATEDIFF(day,i.TranDate,CURRENT_TIMESTAMP) &lt;= 30 THEN '&lt;=30'
		WHEN DATEDIFF(day,i.TranDate,CURRENT_TIMESTAMP) &lt;= 60 THEN '31-60'
		WHEN DATEDIFF(day,i.TranDate,CURRENT_TIMESTAMP) &lt;= 90 THEN '61-90'
		WHEN DATEDIFF(day,i.TranDate,CURRENT_TIMESTAMP) &lt;= 120 THEN '91-120'
		WHEN DATEDIFF(day,i.TranDate,CURRENT_TIMESTAMP) &gt; 120 THEN '121+'
	END AS ar_bucket
	,cli.row_source_system

FROM netsuite.invoice i
	 inner join netsuite.client c on i.custbody_client = c.id
	 inner join dbo.Client cli on c.id = cli.row_source_system_key
		AND cli.row_source_system = 'Netsuite'
	INNER JOIN dbo.entity c_entity ON cli.entity_id = c_entity.id
	 inner join netsuite.debtor d on i.entity = d.id
	 inner join dbo.Debtor dbt on d.id = dbt.row_source_system_key
		AND dbt.row_source_system = 'Netsuite'
	INNER JOIN dbo.entity ent ON dbt.entity_id = ent.id

WHERE foreignamountunpaid &lt;&gt; 0</property>
